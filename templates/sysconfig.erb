<%- if @service_name == 'unkafka' -%>
KAFKA_TOPIC="<%= @service_item %>"
<%- end %>
<%- if @s3_secret_key %>
S3_ACCESS_SECRET="<%= @s3_secret_key %>"
<%- end %>
<%- if @s3_access_key %>
S3_ACCESS_KEY="<%= @s3_access_key %>"
<%- end %>
<%- if @port %>
PORT="<%= @port %>"
TORNADO_PORT="${PORT}"
<% end %>
<%- exports = scope.lookupvar('systemd::service_exports') -%>
<%- if exports  %>
<%- exports.each do |key, value| -%>
<%= key.upcase %>="<%= value %>"
<%- end -%>
<%- end %>
